{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "strict": true,
    "outDir": "lib",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "tsBuildInfoFile": "./lib/.tsbuildinfo",
    "esModuleInterop": true,
    "skipLibCheck": false,
    "incremental": true,
    "declaration": true,
    "declarationMap": true,
    "types": []
  },
  "include": [
    "src/**/*"
  ],
}
